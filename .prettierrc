{
  "trailingComma": "all",
  "printWidth": 80,
  "importOrder": ["^react", "^[a-z]", "^[./]"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,

  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
